{
    "plugins": [
        "edit-link",
        "github",
        "theme-api",
        "anchors",
        "katex"
    ],
    "pluginsConfig": {
        "edit-link": {
            "base": "https://github.com/SpoonX/wetland/edit/master/doc",
            "label": "Edit This Page"
        },
        "github": {
            "url": "https://github.com/spoonx/wetland"
        },
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "ts",
                    "name": "TypeScript"
                }
            ],
            "split": true,
            "theme": "dark"
        }
    },
    "root": "./doc"
}